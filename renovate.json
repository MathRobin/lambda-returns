{
  "extends": [
    "config:base"
  ],
  "encrypted": {
    "npmToken": ""
  },
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "ignoreDeps": ["node-fetch", "serverless-offline"],
  "packageRules": [
    {
      "matchManagers": ["nvm"],
      "enabled": false
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePatterns": ["mysql", "node"],
      "enabled": false
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePatterns": ["mysql", "node"],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": ["patch"],
      "groupName": "all patch dependencies",
      "groupSlug": "all-patch",
      "automerge": true
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor"
      ],
      "matchDepTypes": ["devDependencies"],
      "groupName": "all minor devDependencies",
      "groupSlug": "all-minor",
      "automerge": true
    }
  ]
}

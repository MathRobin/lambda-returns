{
  "name": "lambda-returns",
  "version": "2.1.0",
  "main": "index.cjs",
  "module": "index.esm.js",
  "types": "./index.esm.d.ts",
  "repository": "git@github.com:MathRobin/lambda-returns.git",
  "author": "MathRobin <mthrobin@gmail.com>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "node build.js; npx -p typescript tsc ./index.esm.js --declaration --allowJs --emitDeclarationOnly --outDir .",
    "lint": "eslint . && eslint -c .eslintrc.tests.yml",
    "testCjs": "jest --coverage -c jest.config.cjs --coverageDirectory coverage_cjs",
    "testEsm": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage -c jest.config.esm.js",
    "pretest": "yarn build",
    "test": "yarn testCjs && yarn testEsm",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-security": "1.5.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "dependencies": {}
}

{
  "name": "lambda-returns",
  "version": "4.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "repository": "git@github.com:MathRobin/lambda-returns.git",
  "author": "MathRobin <mthrobin@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "engines": {
    "node": "18.18.2"
  },
  "scripts": {
    "build": "tsx ./generate.ts && tsx ./generate_test.ts && yarn prettier:write && vite build && mv ./dist/src/* ./dist/ && rm -rf ./dist/vit* ./dist/index.test* ./dist/generate* ./dist/eslint* ./dist/codes* ./dist/src ./dist/random_integer",
    "lint": "eslint -c eslint.config.js",
    "prepare": "husky",
    "prettier": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "publish": "yarn npm publish && git add package.json yarn.lock && git commit -m 'feat(version): bump' && git push && git status",
    "test": "vitest run --reporter=verbose --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/eslint-parser": "7.24.6",
    "@babel/plugin-syntax-import-assertions": "7.24.6",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.3.0",
    "@types/babel__core": "7.20.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.12.12",
    "@vitest/coverage-v8": "1.6.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.3.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "lodash-es": "4.17.21",
    "prettier": "3.2.5",
    "tsx": "4.11.0",
    "typescript-eslint": "7.11.0",
    "vite": "5.2.11",
    "vite-plugin-dts": "3.9.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}

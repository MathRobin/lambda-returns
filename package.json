{
  "name": "lambda-returns",
  "version": "3.0.1",
  "main": "index.cjs",
  "module": "index.js",
  "types": "./index.d.ts",
  "repository": "git@github.com:MathRobin/lambda-returns.git",
  "author": "MathRobin <mthrobin@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "yarn@3.6.4",
  "type": "module",
  "scripts": {
    "build": "yarn build:source && yarn build:typings",
